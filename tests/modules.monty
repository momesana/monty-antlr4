# Preferred namespace for open-source projects.
# module org.productName.class

# Preferred namespace for proprietary software.
# module com.companyName.productName

# Example:
module org.monty.examples

import monty.util.*
import monty.util.{Date, Collection, List}

# Renaming imports.
import monty.util.{Collection as KodCollection}

# Monty favours programming in British English.
import java.awt.{Color as Colour, Graphics}